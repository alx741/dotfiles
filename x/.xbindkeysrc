# Increase Volume
"amixer set Master 5%+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

# Decrease Volume
"amixer set Master 5%-"
    m:0x0 + c:122
    XF86AudioLowerVolume

# Mute
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute

# Play/Pause
"mpc toggle"
    m:0x0 + c:172
    XF86AudioPlay

# Next
"mpc next"
    m:0x0 + c:171
    XF86AudioNext

# Prev
"mpc prev"
    m:0x0 + c:173
    XF86AudioPrev

